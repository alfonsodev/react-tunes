{
  "openapi": "3.0.0",
  "info": {
    "title": "Sample API",
    "description": "Optional multiline or single-line description in [CommonMark](http://commonmark.org/help/) or HTML.",
    "version": "0.1.9"
  },
  "servers": [
    {
      "url": "http://localhost:3000/v1",
      "description": "Your local development server."
    },
    {
      "url": "http://staging-api.heroku.com",
      "description": "Staging server for testing"
    }
  ],
  "paths": {
    "/songs": {
      "get": {
        "summary": "Returns a list of songs.",
        "description": "Endpoints for retrieving information about one or more tracks from the catalog.",
        "responses": {
          "200": {
            "description": "A JSON array of songs",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}